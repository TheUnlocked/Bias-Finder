{
  "manifest_version": 2,

  "name": "Bias Finder",
  "description": "Find the bias of any news site you go to with this simple extension!",
  "version": "0.2.6",

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
      "scripts": ["jquery-3.1.1.min.js", "background.js"]
  }
}
